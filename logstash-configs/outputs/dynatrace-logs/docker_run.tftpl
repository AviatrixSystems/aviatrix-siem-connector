# DOCKER RUN FOR DYNATRACE LOGS OUTPUT
sudo docker run -d --restart=always -v /logstash/pipeline/:/usr/share/logstash/pipeline/ -v /logstash/patterns:/usr/share/logstash/patterns -e LOG_PROFILE=${log_profile} -e DT_LOGS_URL=https://${dt_environment_id}.${dt_region}.dynatrace.com/api/v2/logs/ingest -e DT_LOGS_TOKEN=${dt_logs_token} -e DT_LOG_SOURCE=${dt_log_source} -e XPACK_MONITORING_ENABLED=false -p 5000:5000 docker.elastic.co/logstash/logstash:8.16.2
