# DOCKER RUN FOR DYNATRACE METRICS OUTPUT
sudo docker run -d --restart=always -v /logstash/pipeline/:/usr/share/logstash/pipeline/ -v /logstash/patterns:/usr/share/logstash/patterns -e LOG_PROFILE=${log_profile} -e DT_METRICS_URL=https://${dt_environment_id}.${dt_region}.dynatrace.com/api/v2/metrics/ingest -e DT_API_TOKEN=${dt_api_token} -e DT_METRIC_SOURCE=${dt_metric_source} -e XPACK_MONITORING_ENABLED=false -p 5000:5000 docker.elastic.co/logstash/logstash:8.16.2
